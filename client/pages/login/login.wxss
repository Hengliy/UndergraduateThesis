.zan-dialog__mask {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 10;
  background: rgba(0, 0, 0, 0);
  display: none;
}

.zan-dialog__container {
  position: fixed;
  bottom: 400rpx;
  width: 650rpx;
  height: 350rpx;
  margin-left: 50rpx;
  background: #f8f8f8;
  transform: translateY(300%);
  transition: all 0.4s ease;
  z-index: 12;
  border-radius: 20rpx;
  box-shadow: 0px 3px 3px 2px gainsboro;
}

.zan-dialog--show .zan-dialog__container {
  transform: translateY(0);
}

.zan-dialog--show .zan-dialog__mask {
  display: block;
}

 /* 遮罩层 */
.mask{
  width: 100%;
  height: 100%; /*宽高 100% 铺满屏幕 */
  background: #000;
  opacity: 0.5;
  z-index: 9000;
}
 
.modalDlg{
  width: 90%;
  height: 50%;
  z-index: 9900;  /* 要比遮罩层的大 */
  background-color: #FFA07A;
  border-radius: 30rpx;
  position: fixed; /* 生成绝对定位的元素，相对于浏览器窗口进行定位。 */
  top: 10%;
  left: 5%;  /* (100% - width)/2 */
}


